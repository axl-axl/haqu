/* pages/urlresolved/urlresolved.wxss */
page{
    background:#f5f5f5;
}
.url_box{
    width:calc(100% - 2rem);
    margin:0 1rem;
    border:1px solid #cbcbcb;
    margin-top:1rem;
    background:#fff;
    padding:0.8rem;
    box-sizing:border-box;
    border-radius:5px;
}
.url_placeholder{
    color:#cbcbcb;
    font-size:0.8rem;
}
.url_warning{
    color:#cbcbcb;
    font-size:0.8rem;
    width:calc(100% - 2rem);
    margin:1rem;
}
.url_btn{
    width:50%;
    border-radius:5px;
    background:#cbcbcb;
    color:#fff;
    line-height:2.5rem;
    text-align:center;
    margin:1rem auto;
}